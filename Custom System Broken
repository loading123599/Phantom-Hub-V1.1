--INPUTS
TargetName_Input.FocusLost:Connect(function(enterPressed)
	if enterPressed then
		local targetplr = GetPlayer(TargetName_Input.Text:lower())
		if targetplr then
			TargetedPlayer = targetplr
			TargetImage.Image = Players:GetUserThumbnailAsync(targetplr.UserId,Enum.ThumbnailType.HeadShot,Enum.ThumbnailSize.Size420x420)
			UserIDTargetLabel.Text = "UserID: "..targetplr.UserId
		end
	end
end)

ChatBox_Input.FocusLost:Connect(function(enterPressed)
	if enterPressed then
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(ChatBox_Input.Text, "All")
		ChatBox_Input.Text = ""
	end
end)

CMDBar.FocusLost:Connect(function(enterPressed)
	if enterPressed then
		local cmd = CMDBar.Text:lower()
		if cmd == "antiragdoll" then
			ToggleButton(AntiRagdoll_Button)
		elseif cmd == "pushaura" then
			ToggleButton(PushAura_Button)
		elseif cmd == "spammines" then
			ToggleButton(SpamMines_Button)
		elseif cmd == "potionfling" then
			ToggleButton(PotionFling_Button)
		elseif cmd == "touchfling" then
			ToggleButton(TouchFling_Button)
		elseif cmd == "voidprotection" then
			ToggleButton(VoidProtection_Button)
		elseif cmd == "freepushtool" then
			ToggleButton(FreePushTool_Button)
		elseif cmd == "breakcannons" then
			ToggleButton(BreakCannons_Button)
		elseif cmd == "lethalcannons" then
			ToggleButton(LethalCannons_Button)
		elseif cmd == "chatalert" then
			ToggleButton(ChatAlert_Button)
		elseif cmd == "potiondi" then
			ToggleButton(PotionDi_Button)
		elseif cmd == "pushall" then
			ToggleButton(PushAll_Button)
		elseif cmd == "antifling" then
			ToggleButton(AntiFling_Button)
		elseif cmd == "antichatspy" then
			ToggleButton(AntiChatSpy_Button)
		elseif cmd == "antiafk" then
			ToggleButton(AntiAFK_Button)
		elseif cmd == "shaders" then
			ToggleButton(Shaders_Button)
		elseif cmd == "freeemotes" then
			ToggleButton(FreeEmotes_Button)
		elseif cmd == "fly" then
			ToggleButton(Fly_Button)
		elseif cmd == "viewtarget" then
			ToggleButton(ViewTarget_Button)
		elseif cmd == "flingtarget" then
			ToggleButton(FlingTarget_Button)
		elseif cmd == "focustarget" then
			ToggleButton(FocusTarget_Button)
		elseif cmd == "benxtarget" then
			ToggleButton(BenxTarget_Button)
		elseif cmd == "headsittarget" then
			ToggleButton(HeadsitTarget_Button)
		elseif cmd == "standtarget" then
			ToggleButton(StandTarget_Button)
		elseif cmd == "backpacktarget" then
			ToggleButton(BackpackTarget_Button)
		elseif cmd == "doggytarget" then
			ToggleButton(DoggyTarget_Button)
		elseif cmd == "dragtarget" then
			ToggleButton(DragTarget_Button)
		elseif cmd == "day" then
			Light.TimeOfDay = "12:00:00"
		elseif cmd == "night" then
			Light.TimeOfDay = "00:00:00"
		elseif cmd == "rejoin" then
			game:GetService("TeleportService"):Teleport(game.PlaceId, plr)
		elseif cmd == "cmdx" then
			loadstring(game:HttpGet("https://raw.githubusercontent.com/CMD-X/CMD-X/master/Source", true))()
		elseif cmd == "infyield" then
			loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
		elseif cmd == "serverhop" then
			local x = {}
			for _, v in ipairs(game:GetService("HttpService"):JSONDecode(game:HttpGetAsync("https://games.roblox.com/v1/games/" .. game.PlaceId .. "/servers/Public?sortOrder=Asc&limit=100")).data) do
				if type(v) == "table" and v.maxPlayers > v.playing and v.id ~= game.JobId then
					x[#x + 1] = v.id
				end
			end
			if #x > 0 then
				game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, x[math.random(1, #x)])
			else
				return SendNotify("Serverhop", "Couldn't find a server", 3)
			end
		elseif cmd == "explode" then
			plr.Character.Humanoid.Health = 0
			task.wait(0.1)
			for i,v in pairs(plr.Character:GetChildren()) do
				if v:IsA("BasePart") then
					local ex = Instance.new("Explosion")
					ex.Position = v.Position
					ex.Parent = workspace
				end
			end
		end
		CMDBar.Text = ""
	end
end)

--OPEN/CLOSE GUI
local guiopen = true
local function ToggleGui()
	guiopen = not guiopen
	Background.Visible = guiopen
	OpenClose.Visible = not guiopen
end

mouse.KeyDown:Connect(function(key)
	if key:lower() == "b" then
		ToggleGui()
	end
end)

OpenClose.MouseButton1Click:Connect(function()
	ToggleGui()
end)

--LOOPS
task.spawn(function()
	while task.wait() do
		if Toggles.PushAura then
			for i,v in pairs(Players:GetPlayers()) do
				if v ~= plr and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and (v.Character.HumanoidRootPart.Position - GetRoot(plr).Position).Magnitude < 10 then
					Push(v)
				end
			end
		end
		if Toggles.SpamMines and MinesFolder then
			for i,v in pairs(MinesFolder:GetChildren()) do
				if v:IsA("BasePart") then
					Touch(v,GetRoot(plr))
				end
			end
		end
		if Toggles.PotionFling and CheckPotion() then
			PotionTool.Parent = plr.Character
			PotionTool:Activate()
			task.wait()
			PotionTool.Parent = plr.Backpack
		end
		if Toggles.TouchFling then
			for i,v in pairs(Players:GetPlayers()) do
				if v ~= plr and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and (v.Character.HumanoidRootPart.Position - GetRoot(plr).Position).Magnitude < 5 then
					GetRoot(plr).Velocity = Vector3.new(500,500,500)
					task.wait(0.1)
					GetRoot(plr).Velocity = Vector3.new(0,0,0)
				end
			end
		end
		if Toggles.FreePushTool then
			if not plr.Backpack:FindFirstChild("ModdedPush") and not plr.Character:FindFirstChild("ModdedPush") then
				local tool = Instance.new("Tool")
				tool.Name = "ModdedPush"
				tool.RequiresHandle = false
				tool.Parent = plr.Backpack
				tool.Activated:Connect(function()
					local target = mouse.Target
					if target and target.Parent and target.Parent:FindFirstChild("Humanoid") then
						local targetplr = Players:GetPlayerFromCharacter(target.Parent)
						if targetplr then
							Push(targetplr)
						end
					end
				end)
			end
		end
		if Toggles.BreakCannons then
			for i,v in pairs(CannonsFolders) do
				for i2,v2 in pairs(v:GetDescendants()) do
					if v2:IsA("BasePart") and v2.Name == "TouchInterest" then
						Touch(v2,GetRoot(plr))
					end
				end
			end
		end
		if Toggles.LethalCannons then
			for i,v in pairs(CannonsFolders) do
				for i2,v2 in pairs(v:GetDescendants()) do
					if v2:IsA("BasePart") and v2.Name == "TouchInterest" then
						for i3,v3 in pairs(Players:GetPlayers()) do
							if v3 ~= plr and v3.Character and v3.Character:FindFirstChild("HumanoidRootPart") then
								Touch(v2,GetRoot(v3))
							end
						end
					end
				end
			end
		end
		if Toggles.PotionDi and CheckPotion() then
			PotionTool.Parent = plr.Character
			PotionTool.Handle.CFrame = plr.Character.HumanoidRootPart.CFrame * CFrame.new(0,0,-1)
		end
		if Toggles.PushAll then
			for i,v in pairs(Players:GetPlayers()) do
				if v ~= plr and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
					Push(v)
				end
			end
		end
		if Toggles.AntiChatSpy then
			for i,v in pairs(getconnections(game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.OnMessageDoneFiltering.OnClientEvent)) do
				v:Disable()
			end
		end
		if Toggles.AntiAFK then
			for i,v in pairs(getconnections(plr.Idled)) do
				v:Disable()
			end
		end
		if Toggles.Shaders then
			if not Light:FindFirstChild("ColorCorrection") then
				local cc = Instance.new("ColorCorrectionEffect")
				cc.Name = "ColorCorrection"
				cc.Brightness = 0.1
				cc.Contrast = 0.5
				cc.Saturation = 0.5
				cc.TintColor = Color3.fromRGB(255, 255, 255)
				cc.Parent = Light
			end
			if not Light:FindFirstChild("SunRays") then
				local sr = Instance.new("SunRaysEffect")
				sr.Name = "SunRays"
				sr.Intensity = 0.1
				sr.Spread = 1
				sr.Parent = Light
			end
			if not Light:FindFirstChild("Bloom") then
				local bl = Instance.new("BloomEffect")
				bl.Name = "Bloom"
				bl.Intensity = 0.1
				bl.Size = 24
				bl.Threshold = 2
				bl.Parent = Light
			end
		else
			if Light:FindFirstChild("ColorCorrection") then
				Light.ColorCorrection:Destroy()
			end
			if Light:FindFirstChild("SunRays") then
				Light.SunRays:Destroy()
			end
			if Light:FindFirstChild("Bloom") then
				Light.Bloom:Destroy()
			end
		end
		if Toggles.FreeEmotes then
			for i,v in pairs(plr.PlayerGui:GetDescendants()) do
				if v:IsA("Frame") and v.Name == "Emotes" then
					for i2,v2 in pairs(v:GetDescendants()) do
						if v2:IsA("TextButton") and v2:FindFirstChild("EmoteName") then
							v2.EmoteName.Value = "headless"
						end
					end
				end
			end
		end
		if Toggles.Fly then
			local flyvel = Vector3.new(0,0,0)
			if UserInputService:IsKeyDown(Enum.KeyCode.W) then
				flyvel = flyvel + (workspace.CurrentCamera.CFrame.LookVector * FlySpeed)
			end
			if UserInputService:IsKeyDown(Enum.KeyCode.S) then
				flyvel = flyvel - (workspace.CurrentCamera.CFrame.LookVector * FlySpeed)
			end
			if UserInputService:IsKeyDown(Enum.KeyCode.A) then
				flyvel = flyvel - (workspace.CurrentCamera.CFrame.RightVector * FlySpeed)
			end
			if UserInputService:IsKeyDown(Enum.KeyCode.D) then
				flyvel = flyvel + (workspace.CurrentCamera.CFrame.RightVector * FlySpeed)
			end
			if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
				flyvel = flyvel + (workspace.CurrentCamera.CFrame.UpVector * FlySpeed)
			end
			if UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then
				flyvel = flyvel - (workspace.CurrentCamera.CFrame.UpVector * FlySpeed)
			end
			if GetRoot(plr) then
				GetRoot(plr).Velocity = flyvel
			end
		end
		if Toggles.ViewTarget and TargetedPlayer and TargetedPlayer.Character then
			workspace.CurrentCamera.CameraSubject = TargetedPlayer.Character.Humanoid
		else
			workspace.CurrentCamera.CameraSubject = plr.Character.Humanoid
		end
		if Toggles.FlingTarget and TargetedPlayer and TargetedPlayer.Character and TargetedPlayer.Character:FindFirstChild("HumanoidRootPart") then
			PredictionTP(TargetedPlayer,"safe")
		end
		if Toggles.FocusTarget and TargetedPlayer and TargetedPlayer.Character and TargetedPlayer.Character:FindFirstChild("HumanoidRootPart") then
			TeleportTO(0,0,0,TargetedPlayer,"safe")
		end
		if Toggles.BenxTarget and TargetedPlayer and TargetedPlayer.Character and TargetedPlayer.Character:FindFirstChild("HumanoidRootPart") then
			GetRoot(plr).CFrame = CFrame.new(GetRoot(TargetedPlayer).Position) * CFrame.Angles(0,math.rad(GetRoot(plr).Orientation.Y+5),0)
		end
		if Toggles.HeadsitTarget and TargetedPlayer and TargetedPlayer.Character and TargetedPlayer.Character:FindFirstChild("HumanoidRootPart") then
			GetRoot(plr).CFrame = CFrame.new(GetRoot(TargetedPlayer).Position) * CFrame.new(0,3.5,0)
		end
		if Toggles.StandTarget and TargetedPlayer and TargetedPlayer.Character and TargetedPlayer.Character:FindFirstChild("HumanoidRootPart") then
			GetRoot(plr).CFrame = CFrame.new(GetRoot(TargetedPlayer).Position) * CFrame.new(0,5,0)
		end
		if Toggles.BackpackTarget and TargetedPlayer and TargetedPlayer.Character and TargetedPlayer.Character:FindFirstChild("HumanoidRootPart") then
			GetRoot(plr).CFrame = CFrame.new(GetRoot(TargetedPlayer).Position) * CFrame.new(0,0,1.5)
		end
		if Toggles.DoggyTarget and TargetedPlayer and TargetedPlayer.Character and TargetedPlayer.Character:FindFirstChild("HumanoidRootPart") then
			GetRoot(plr).CFrame = CFrame.new(GetRoot(TargetedPlayer).Position) * CFrame.new(0,0,-1.5)
		end
		if Toggles.DragTarget and TargetedPlayer and TargetedPlayer.Character and TargetedPlayer.Character:FindFirstChild("HumanoidRootPart") then
			GetRoot(TargetedPlayer).CFrame = CFrame.new(GetRoot(plr).Position) * CFrame.new(0,0,3)
		end
		if SavedCheckpoint then
			if plr.Character.Humanoid.Health <= 0 then
				task.wait(1)
				GetRoot(plr).CFrame = CFrame.new(SavedCheckpoint)
			end
		end
	end
end)

