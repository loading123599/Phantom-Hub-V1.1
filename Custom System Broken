--ANTI FLING
task.spawn(function()
	while task.wait() do
		if _G.AntiFlingToggled then
			for i,v in pairs(Players:GetPlayers()) do
				if v ~= plr and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and (v.Character.HumanoidRootPart.Position - GetRoot(plr).Position).Magnitude < 10 then
					for i2,v2 in pairs(v.Character:GetDescendants()) do
						if v2:IsA("BasePart") then
							v2.CanCollide = false
						end
					end
				end
			end
		end
	end
end)

--CHAT ALERT
task.spawn(function()
	while task.wait() do
		if Toggles.ChatAlert then
			for i,v in pairs(Players:GetPlayers()) do
				if v ~= plr then
					v.Chatted:Connect(function(msg)
						SendNotify(v.Name, msg, 3)
					end)
				end
			end
		end
	end
end)

--NOTIFY
SendNotify("Poison Hub", "GUI Loaded!", 3)
--FUNCTIONS
function ChangeToggleColor(Button)
	if Button:FindFirstChild("UIGradient") then
		if Button.UIGradient.Color == ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(128, 0, 255)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(0, 0, 0))} then
			Button.UIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(0, 0, 0)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(0, 0, 0))}
		else
			Button.UIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(128, 0, 255)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(0, 0, 0))}
		end
	end
end

function CreateToggle(Button)
	local UIGradient = Instance.new("UIGradient")
	UIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(0, 0, 0)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(0, 0, 0))}
	UIGradient.Parent = Button
end

function CreateClicker(Button)
	local UIGradient = Instance.new("UIGradient")
	UIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(0, 0, 0)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(0, 0, 0))}
	UIGradient.Parent = Button
	
	Button.MouseButton1Down:Connect(function()
		UIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(128, 0, 255)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(0, 0, 0))}
	end)
	
	Button.MouseButton1Up:Connect(function()
		UIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(0, 0, 0)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(0, 0, 0))}
	end)
end

function SendNotify(Title, Text, Duration)
	game.StarterGui:SetCore("SendNotification", {
		Title = Title,
		Text = Text,
		Duration = Duration
	})
end

function GetPlayer(String)
	for i,v in pairs(Players:GetPlayers()) do
		if v.Name:lower():sub(1, #String) == String:lower() or v.DisplayName:lower():sub(1, #String) == String:lower() then
			return v
		end
	end
	return nil
end

function GetRoot(Player)
	if Player.Character and Player.Character:FindFirstChild("HumanoidRootPart") then
		return Player.Character.HumanoidRootPart
	end
	return nil
end

function ToggleRagdoll(Toggle)
	if Toggle then
		for i,v in pairs(plr.Character:GetDescendants()) do
			if v:IsA("BallSocketConstraint") or v:IsA("HingeConstraint") then
				v.Enabled = false
			end
		end
	else
		for i,v in pairs(plr.Character:GetDescendants()) do
			if v:IsA("BallSocketConstraint") or v:IsA("HingeConstraint") then
				v.Enabled = true
			end
		end
	end
end

function ToggleVoidProtection(Toggle)
	if Toggle then
		if not plr.Character:FindFirstChild("VoidProtection") then
			local part = Instance.new("Part")
			part.Name = "VoidProtection"
			part.Size = Vector3.new(500, 3, 500)
			part.Anchored = true
			part.CanCollide = true
			part.Transparency = 0.5
			part.Position = Vector3.new(0, -50, 0)
			part.Parent = plr.Character
		end
	else
		if plr.Character:FindFirstChild("VoidProtection") then
			plr.Character.VoidProtection:Destroy()
		end
	end
end

function Push(Target)
	if Target.Character and Target.Character:FindFirstChild("HumanoidRootPart") then
		local args = {
			[1] = Target.Character.HumanoidRootPart
		}
		
		game:GetService("ReplicatedStorage").Events.PushEvent:FireServer(unpack(args))
	end
end

function Touch(Part1, Part2)
	firetouchinterest(Part1, Part2, 0)
	task.wait()
	firetouchinterest(Part1, Part2, 1)
end

function CheckPotion()
	for i,v in pairs(plr.Backpack:GetChildren()) do
		if v:IsA("Tool") and v.Name == "Potion" then
			PotionTool = v
			return true
		end
	end
	for i,v in pairs(plr.Character:GetChildren()) do
		if v:IsA("Tool") and v.Name == "Potion" then
			PotionTool = v
			return true
		end
	end
	return false
end

function TeleportTO(x, y, z, Target, Mode)
	if Mode == "safe" then
		if Target == "pos" then
			GetRoot(plr).CFrame = CFrame.new(x, y, z)
		else
			if Target.Character and Target.Character:FindFirstChild("HumanoidRootPart") then
				GetRoot(plr).CFrame = CFrame.new(GetRoot(Target).Position)
			end
		end
	else
		if Target == "pos" then
			GetRoot(plr).Position = Vector3.new(x, y, z)
		else
			if Target.Character and Target.Character:FindFirstChild("HumanoidRootPart") then
				GetRoot(plr).Position = GetRoot(Target).Position
			end
		end
	end
end

function PredictionTP(Target, Mode)
	if Target.Character and Target.Character:FindFirstChild("HumanoidRootPart") then
		local TargetVel = GetRoot(Target).Velocity
		local PredictedPosition = GetRoot(Target).Position + (TargetVel * 0.1)
		if Mode == "safe" then
			GetRoot(plr).CFrame = CFrame.new(PredictedPosition)
		else
			GetRoot(plr).Position = PredictedPosition
		end
	end
end

function PlayAnim(ID, Speed, Priority)
	if plr.Character and plr.Character:FindFirstChild("Humanoid") then
		local Anim = Instance.new("Animation")
		Anim.AnimationId = "rbxassetid://"..ID
		local Track = plr.Character.Humanoid:LoadAnimation(Anim)
		Track:Play()
		Track:AdjustSpeed(Speed)
		Track.Priority = Enum.AnimationPriority[Priority]
	end
end

--SHIELD FUNCTION
_G.shield = function(id)
	local shieldedPlayers = {}
	table.insert(shieldedPlayers, id)
	
	local mt = getrawmetatable(game)
	local old = mt.__namecall
	setreadonly(mt, false)
	
	mt.__namecall = newcclosure(function(self, ...)
		local args = {...}
		local method = getnamecallmethod()
		
		if method == "FireServer" and tostring(self) == "PushEvent" then
			local hrp = args[1]
			if hrp and hrp.Parent and hrp.Parent:FindFirstChild("Humanoid") then
				local character = hrp.Parent
				local player = Players:GetPlayerFromCharacter(character)
				if player and table.find(shieldedPlayers, player.UserId) then
					return
				end
			end
		end
		
		return old(self, ...)
	end)
	
	setreadonly(mt, true)
end

--WELCOME MESSAGE
local welcomeMessage = [[
Welcome to Poison Hub!

Press B to toggle the GUI.
]]

SendNotify("Poison Hub", welcomeMessage, 5)
