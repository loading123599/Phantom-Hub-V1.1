--TOGGLES
local Toggles = {
	AntiRagdoll = false,
	PushAura = false,
	SpamMines = false,
	PotionFling = false,
	TouchFling = false,
	VoidProtection = false,
	FreePushTool = false,
	BreakCannons = false,
	LethalCannons = false,
	ChatAlert = false,
	PotionDi = false,
	PushAll = false,
	AntiFling = false,
	AntiChatSpy = false,
	AntiAFK = false,
	Shaders = false,
	FreeEmotes = false,
	Fly = false,
	FlyPad = false,
	ViewTarget = false,
	FlingTarget = false,
	FocusTarget = false,
	BenxTarget = false,
	HeadsitTarget = false,
	StandTarget = false,
	BackpackTarget = false,
	DoggyTarget = false,
	DragTarget = false
}

--FUNCTIONS
local function ToggleButton(Button)
	if Button.Name == "AntiRagdoll_Button" then
		Toggles.AntiRagdoll = not Toggles.AntiRagdoll
		ToggleRagdoll(Toggles.AntiRagdoll)
		ChangeToggleColor(Button)
	elseif Button.Name == "PushAura_Button" then
		Toggles.PushAura = not Toggles.PushAura
		ChangeToggleColor(Button)
	elseif Button.Name == "SpamMines_Button" then
		Toggles.SpamMines = not Toggles.SpamMines
		ChangeToggleColor(Button)
	elseif Button.Name == "PotionFling_Button" then
		Toggles.PotionFling = not Toggles.PotionFling
		ChangeToggleColor(Button)
	elseif Button.Name == "TouchFling_Button" then
		Toggles.TouchFling = not Toggles.TouchFling
		ChangeToggleColor(Button)
	elseif Button.Name == "VoidProtection_Button" then
		Toggles.VoidProtection = not Toggles.VoidProtection
		ToggleVoidProtection(Toggles.VoidProtection)
		ChangeToggleColor(Button)
	elseif Button.Name == "FreePushTool_Button" then
		Toggles.FreePushTool = not Toggles.FreePushTool
		ChangeToggleColor(Button)
	elseif Button.Name == "BreakCannons_Button" then
		Toggles.BreakCannons = not Toggles.BreakCannons
		ChangeToggleColor(Button)
	elseif Button.Name == "LethalCannons_Button" then
		Toggles.LethalCannons = not Toggles.LethalCannons
		ChangeToggleColor(Button)
	elseif Button.Name == "ChatAlert_Button" then
		Toggles.ChatAlert = not Toggles.ChatAlert
		ChangeToggleColor(Button)
	elseif Button.Name == "PotionDi_Button" then
		Toggles.PotionDi = not Toggles.PotionDi
		ChangeToggleColor(Button)
	elseif Button.Name == "PushAll_Button" then
		Toggles.PushAll = not Toggles.PushAll
		ChangeToggleColor(Button)
	elseif Button.Name == "AntiFling_Button" then
		Toggles.AntiFling = not Toggles.AntiFling
		_G.AntiFlingToggled = Toggles.AntiFling
		ChangeToggleColor(Button)
	elseif Button.Name == "AntiChatSpy_Button" then
		Toggles.AntiChatSpy = not Toggles.AntiChatSpy
		ChangeToggleColor(Button)
	elseif Button.Name == "AntiAFK_Button" then
		Toggles.AntiAFK = not Toggles.AntiAFK
		ChangeToggleColor(Button)
	elseif Button.Name == "Shaders_Button" then
		Toggles.Shaders = not Toggles.Shaders
		ChangeToggleColor(Button)
	elseif Button.Name == "FreeEmotes_Button" then
		Toggles.FreeEmotes = not Toggles.FreeEmotes
		FreeEmotesEnabled = Toggles.FreeEmotes
		ChangeToggleColor(Button)
	elseif Button.Name == "Fly_Button" then
		Toggles.Fly = not Toggles.Fly
		Toggles.FlyPad = Toggles.Fly
		Fly_Pad.Visible = Toggles.FlyPad
		ChangeToggleColor(Button)
	elseif Button.Name == "ViewTarget_Button" then
		Toggles.ViewTarget = not Toggles.ViewTarget
		ChangeToggleColor(Button)
	elseif Button.Name == "FlingTarget_Button" then
		Toggles.FlingTarget = not Toggles.FlingTarget
		ChangeToggleColor(Button)
	elseif Button.Name == "FocusTarget_Button" then
		Toggles.FocusTarget = not Toggles.FocusTarget
		ChangeToggleColor(Button)
	elseif Button.Name == "BenxTarget_Button" then
		Toggles.BenxTarget = not Toggles.BenxTarget
		ChangeToggleColor(Button)
	elseif Button.Name == "HeadsitTarget_Button" then
		Toggles.HeadsitTarget = not Toggles.HeadsitTarget
		ChangeToggleColor(Button)
	elseif Button.Name == "StandTarget_Button" then
		Toggles.StandTarget = not Toggles.StandTarget
		ChangeToggleColor(Button)
	elseif Button.Name == "BackpackTarget_Button" then
		Toggles.BackpackTarget = not Toggles.BackpackTarget
		ChangeToggleColor(Button)
	elseif Button.Name == "DoggyTarget_Button" then
		Toggles.DoggyTarget = not Toggles.DoggyTarget
		ChangeToggleColor(Button)
	elseif Button.Name == "DragTarget_Button" then
		Toggles.DragTarget = not Toggles.DragTarget
		ChangeToggleColor(Button)
	end
end

