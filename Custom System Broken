--CLICK BUTTONS
WalkSpeed_Button.MouseButton1Click:Connect(function()
	local speed = tonumber(WalkSpeed_Input.Text)
	if speed then
		plr.Character.Humanoid.WalkSpeed = speed
	end
end)

JumpPower_Button.MouseButton1Click:Connect(function()
	local power = tonumber(JumpPower_Input.Text)
	if power then
		plr.Character.Humanoid.JumpPower = power
	end
end)

FlySpeed_Button.MouseButton1Click:Connect(function()
	local speed = tonumber(FlySpeed_Input.Text)
	if speed then
		FlySpeed = speed
	end
end)

SaveCheckpoint_Button.MouseButton1Click:Connect(function()
	SavedCheckpoint = GetRoot(plr).Position
	SendNotify("Checkpoint", "Checkpoint saved!", 3)
end)

ClearCheckpoint_Button.MouseButton1Click:Connect(function()
	SavedCheckpoint = nil
	SendNotify("Checkpoint", "Checkpoint cleared!", 3)
end)

Respawn_Button.MouseButton1Click:Connect(function()
	plr.Character:BreakJoints()
end)

ClickTargetTool_Button.MouseButton1Click:Connect(function()
	local tool = Instance.new("Tool")
	tool.Name = "ClickTarget"
	tool.RequiresHandle = false
	tool.Parent = plr.Backpack
	tool.Activated:Connect(function()
		local target = mouse.Target
		if target and target.Parent and target.Parent:FindFirstChild("Humanoid") then
			local targetplr = Players:GetPlayerFromCharacter(target.Parent)
			if targetplr then
				TargetedPlayer = targetplr
				TargetName_Input.Text = targetplr.Name
				TargetImage.Image = Players:GetUserThumbnailAsync(targetplr.UserId,Enum.ThumbnailType.HeadShot,Enum.ThumbnailSize.Size420x420)
				UserIDTargetLabel.Text = "UserID: "..targetplr.UserId
			end
		end
	end)
end)

PushTarget_Button.MouseButton1Click:Connect(function()
	if TargetedPlayer then
		Push(TargetedPlayer)
	end
end)

WhitelistTarget_Button.MouseButton1Click:Connect(function()
	if TargetedPlayer then
		_G.shield(TargetedPlayer.UserId)
		SendNotify("Whitelist", TargetedPlayer.Name.." has been whitelisted!", 3)
	end
end)

TeleportTarget_Button.MouseButton1Click:Connect(function()
	if TargetedPlayer then
		TeleportTO(0,0,0,TargetedPlayer,"safe")
	end
end)

